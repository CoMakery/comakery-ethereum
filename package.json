{
  "name": "comakery-ethereum",
  "version": "0.0.0",
  "description": "CoMakery Ethereum contracts and infrastructure",
  "author": "Harlan T Wood (https://github.com/harlantwood)",
  "contributors": [
    "Noah Thorp (https://github.com/aquabu)"
  ],
  "scripts": {
    "start": "nodemon --ignore . bin/server",
    "dev:server": "DEBUG=config,dev,server,token nodemon --watch lib --watch environments bin/server",
    "dev": "DEBUG=config,dev,server,token bin/dev",
    "truffle": "truffle",
    "testrpc": "testrpc -p 7777",
    "fulltest": "npm-run-all --silent test lint check-yaml check-for-only",
    "tdd": "npm-run-all --silent test eslint",
    "test": "truffle test | grep -v '      Contract:'",
    "lint": "coffeelint --quiet lib && npm-run-all --silent eslint",
    "eslint": "eslint test/*",
    "check-yaml": "(js-yaml circle.yml && js-yaml .eslintrc.yml) > /dev/null",
    "shipit": "npm-run-all --silent pre-push && git push --tags origin head",
    "pre-push": "npm-run-all --silent check-clean fulltest",
    "check-clean": "if [[ $(git status --porcelain) ]]; then echo '*** Please stash or commit changes first ***\n' && exit 1; fi ",
    "check-for-only": "if [[ $(grep -r 'contractItOnly(' test) ]]; then echo '*** contractItOnly( *** found in test, please remove' && exit 1; fi",
    "wrap": "npm prune && npm shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:comakery/comakery-ethereum.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/comakery/comakery-ethereum/issues"
  },
  "homepage": "https://github.com/comakery/comakery-ethereum#readme",
  "dependencies": {
    "airbrake": "^1.0.1",
    "body-parser": "^1.15.1",
    "coffee-script": "^1.10.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "lightsaber": "^0.5.3",
    "lodash": "^4.12.0",
    "truffle": "^1.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "coffeelint": "^1.15.7",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "eslint": "^2.9.0",
    "ethereumjs-testrpc": "^2.0.7",
    "js-yaml": "^3.6.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^1.8.0"
  },
  "engines": {
    "node": "~5.6.0"
  }
}
